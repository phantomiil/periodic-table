define slsh_main () { %{{{
  variable gray=0, info=1, title=1;
  variable output = NULL;
  variable default = "pte.pdf";
  variable i;
  _for i (1,__argc-1,1) {
    if (char(__argv[i][0]) == "-") {
      if      (__argv[i] == "-g") gray=1;
      else if (__argv[i] == "-i") info=1;
      else if (__argv[i] == "-t") title=1;
      else if (__argv[i] == "-ng") gray=0;
      else if (__argv[i] == "-ni") info=0;
      else if (__argv[i] == "-nt") title=0;
      else vmessage ("Unkown option '%s' ignored", __argv[i]);
    } else {
      if (output == NULL) output = __argv[i];
    }
  }
  output = (output == NULL) ? default : output;

  prepare(gray);
  
  variable xworld = 19*xcell, yworld=10*ycell;
  variable PLOT = xfig_plot_new(xworld, yworld);
  xfig_plot_set_point_size(PLOT,50);
  PLOT.world(0,xworld,0,yworld);
  PLOT.axes(;off);

  _for i (0,length(element)-1,1) {
    add_element(PLOT, i);
  }
  add_insert(PLOT);
  period(PLOT);
  if (info) {
    groups(PLOT);
  }
  ifnot (gray) {
    color_code(PLOT);
  }
  legend(PLOT);
  PLOT.add_object(info_text("Key", 8; color="pse_info", depth=10, rotate=90), 0, 1.1*ycell,0,0);
  PLOT.add_object(info_text("\\hspace{0.5cm}Periodic Table\n\n\\hspace{1cm} of the\n\nElements", 25; color="pse_info", depth=10), 0.2*xworld,0.85*yworld,-0.5,-0.5);
  PLOT.add_object(xfig_new_polyline([0,0,1.05*xworld,1.05*xworld],[0,1.1*yworld,1.1*yworld,0]; closed, color="pse_base", width=20), -0.025*xworld,-0.025*yworld,-0.5,-0.5);

  PLOT.render(output);
} %}}}