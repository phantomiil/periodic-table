% -*- mode: SLANG; mode: fold; -*- %

require("xfig.sl");

% define colors
private define prepare (gray) {
  if (gray) {
    xfig_new_color("pse_base", 0x000000);
    xfig_new_color("pse_border", 0x000000);
    xfig_new_color("pse_background", 0xffffff);
    xfig_new_color("pse_info", 0x000000);
    xfig_new_color("pse_hydrogen", 0xffffff);
    xfig_new_color("pse_noble_gas", 0xffffff);
    xfig_new_color("pse_alkali_metal", 0xffffff);
    xfig_new_color("pse_alkaline_earth_metal", 0xffffff);
    xfig_new_color("pse_transition_metal", 0xffffff);
    xfig_new_color("pse_post_transition_metal", 0xffffff);
    xfig_new_color("pse_metalloid", 0xffffff);
    xfig_new_color("pse_nonmetal", 0xffffff);
    xfig_new_color("pse_halogen", 0xffffff);
    xfig_new_color("pse_lanthanide", 0xffffff);
    xfig_new_color("pse_actinide", 0xffffff);
    xfig_new_color("pse_transactinide", 0xffffff);
    xfig_new_color("pse_unknown", 0xffffff);
  } else {
    xfig_new_color("pse_base", 0x2a2a2a);
    xfig_new_color("pse_border", 0x2a2a2a);
    xfig_new_color("pse_background", 0x2a2a2a);
    xfig_new_color("pse_info", 0xf3f2f2);
    xfig_new_color("pse_hydrogen", 0x7ab1fc);
    xfig_new_color("pse_noble_gas", 0x30d6c7);
    xfig_new_color("pse_alkali_metal", 0xd6b933);
    xfig_new_color("pse_alkaline_earth_metal", 0xc88b2e);
    xfig_new_color("pse_transition_metal", 0x1d47cb);
    xfig_new_color("pse_post_transition_metal", 0x523deb);
    xfig_new_color("pse_metalloid", 0xad1953);
    xfig_new_color("pse_nonmetal", 0xca2c38);
    xfig_new_color("pse_halogen", 0x5db961);
    xfig_new_color("pse_lanthanide", 0xb746c8);
    xfig_new_color("pse_actinide", 0x9146c8);
    xfig_new_color("pse_transactinide", 0xf2da12);
    xfig_new_color("pse_unknown", 0x6d6d6d);
  }
  
  % redefine png & pdf driver
  variable png = "fig2dev -L png %F %I %O";
  png = strreplace(png, "%F", sprintf("-g \\#%x", xfig_lookup_color_rgb("pse_background")));
  xfig_set_output_driver("png", png);
  variable pdf = "fig2dev -L pdf %F %I %O";
  pdf = strreplace(pdf, "%F", sprintf("-g \\#%x", xfig_lookup_color_rgb("pse_background")));
  xfig_set_output_driver("pdf", pdf);
  % change paper size & font
  xfig_set_paper_size("A4");
  xfig_set_font_style("\sffamily\bfseries"R);
}

