define add_element(plot, z) {
  variable cell = xfig_plot_new(xcell, ycell);
  cell.world(0,xcell,0,ycell);
  cell.axes(;off);
  cell.add_object(xfig_new_polyline([0,0,xcell,xcell],[0,ycell,ycell,0]; closed, fillcolor=ecols[element[z].classification], color="pse_border", depth=100, width=20), 0,0,-0.5,-0.5);
  cell.add_object(state_sym(element[z].state, element[z].radio_active), 17,1,-0.5,-0.5);
  cell.add_object(info_text(sprintf("%s"R, element[z].symbol), 16; depth=10, color="pse_base"), 3, 5, -0.5,-0.5);
  cell.add_object(info_text(sprintf("$\mathbf{%d}$"R, element[z].number), 5; right, depth=10), 4.5,4.7,0.5,-0.5);
  cell.add_object(info_text(sprintf("%s"R, element[z].name), 4; depth=10, color="pse_base"), 2,2,-0.5,-0.5);
  cell.add_object(info_text(sprintf("$%s$"R, element[z].electron_configuration), 5; right, depth=10, color="pse_base"), 20,18,0.5,-0.5);
  variable amass = (element[z].atomic_mass == NULL) ? "---"R : sprintf("$%.3f$", element[z].atomic_mass);
  cell.add_object(info_text(amass, 5; depth=10, color="pse_base"), 1,22,-0.5,-0.5);

  cell.add_object(info_text(oxidation(element[z].oxidation_stages), 4; depth=10, color="pse_base"), 1,15,-0.5,-0.5);
  variable nni = (element[z].number_natural_isotopes == NULL) ? "---" : sprintf("$\mathbf{%d}$"R, element[z].number_natural_isotopes);
  cell.add_object(info_text(nni, 5; right, depth=10, color="pse_base"), 20,22,0.5,-0.5);

  cell.add_object(info_text(isotopes(element[z].mass_number, element[z].abundances), 2; depth=10, color="pse_base"), 1.5,20.8,-0.5,-0.5);

  cell.add_object(info_text(temperature(element[z].transition_point), 4; depth=10, color="pse_base"), 1,13,-0.5,-0.5);

  variable eneg = (element[z].electronegativity == NULL) ? "---" : sprintf("$\mathbf{%.1f}$"R, element[z].electronegativity);
  cell.add_object(info_text(eneg, 4; depth=10, right, color="pse_base"), 20,10,0.5,-0.5);

  variable ien = (element[z].ionisation_energy == NULL) ? "---" : sprintf("$\mathbf{%.3f}$"R, element[z].ionisation_energy);
  cell.add_object(info_text(ien, 4; depth=10, right, color="pse_base"), 20, 8,0.5,-0.5);
  plot.add_object(cell, pos_list[z][0], pos_list[z][1], -0.5, -0.5);
}

