define val2roman (val) {
  if (typeof(val) != Integer_Type) {
    vmessage("Only Int type can be converted");
    return;
  }
  variable ret = "";
  if (val == 0) return "0";
  if (val < 0) {
    ret = "-";
    val = -val;
  }
  variable final = 0;
  variable c = 0;
  variable rnum = [1000, 900, 500, 400, 100, 90, 50, 40, 10, 9, 5, 4, 1];
  variable rlit = ["M", "CM", "D", "CD", "C", "XC", "L", "XL", "X", "IX", "V", "IV", "I"];
  while (final < val) {
    if ((final+rnum[c]) <= val) {
      ret+=rlit[c];
      final+=rnum[c];
    } else {
      c++;
    }
  }
  return ret;
}

private define state_sym (phase, radioac) {
  variable sym = xfig_plot_new(1.25, 1.25);
  sym.world(0,1.25,0,1.25);
  sym.axes(;off);
  variable scol = qualifier("color", "pse_base");
  if (phase == SOLID) {
    sym.add_object(xfig_new_polyline([0,0,1,1],[0,1,1,0]; closed, fillcolor=scol, color=scol, depth=10), 0,0,-0.5,-0.5);
  } else if (phase == LIQUID) {
    sym.add_object(xfig_new_polyline([[0:1:0.05],1,1,0],[0.2*sin([0:1:0.05]*2*PI)+0.6,0.6,1,1]; closed, fillcolor=scol, color=scol, depth=10), 0,0.4,-0.5,-0.5);
    sym.add_object(xfig_new_polyline([[0:1:0.05],1,1,0],[0.2*sin([0:1:0.05]*2*PI)+0.4,0.4,0,0]; closed, fillcolor=scol, color=scol, depth=10), 0,0,-0.5,-0.5);
  } else if (phase == GAS) {
    sym.add_object(xfig_new_polyline([0,0,1,1,0,0.2,0.8,0.8,0.2,0.2],[0,1,1,0,0,0.2,0.2,0.8,0.8,0.2]; closed, fillcolor=scol, color=scol, depth=10), 0,0,-0.5,-0.5);
  }
  if (radioac == UNSTABLE) {
    sym.add_object(xfig_new_polyline([0,0],[0,1]; color=scol, width=10, depth=10), 1.15,0,-0.5,-0.5);
  }
  sym.scale(3);
  return sym;
}

private define info_text (text, size) {
  variable tfield = xfig_plot_new(1,1);
  tfield.world(0,1,0,1);
  tfield.axes(;off);
  variable align = (qualifier_exists("right")) ? 1. : 0.;
  tfield.xylabel(align,0,text,align-0.5,-0.5;; __qualifiers);
  tfield.scale(size);
  return tfield;
}

private define oxidation (vals) {
  if (vals == NULL) { return "---";}
  vals = vals[array_sort(vals; dir=-1)];
  variable i;
  variable ox = "";
  _for i (0, length(vals)-1,1) {
    ox += val2roman(vals[i]);
    if (i<length(vals)-1) {ox += ",";}
  }
  return ox;
}

private define isotopes (is, ab) {
  if (is == NULL) {
    return "---";
  }
  variable i, str="";
  _for i (0,length(is)-1,1) {
    str += sprintf("%d\,(%.3f)"R, is[i], 100*ab[i]);
    if (i < length(is)-1) {
      str+=",";
    }
  }
  return str;
}

private define temperature (points) {
  variable str1 = "-";
  variable str2 = "-";
  if (points != NULL) {
    if (points[0] > 0 and points[0] < 99) str1 = sprintf("%.2f", points[0]);
    else if (points[0] > 0) str1 = sprintf("%.0f", points[0]);
    if (points[1] > 0 and points[1] < 99) str2 = sprintf("%.2f", points[1]);
    else if (points[1] > 0) str2 = sprintf("%.0f", points[1]);
  }
  return sprintf("$\mathbf{%s/%s}$"R, str1, str2);
}

